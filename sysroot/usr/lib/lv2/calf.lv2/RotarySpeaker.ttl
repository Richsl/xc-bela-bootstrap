@prefix : <http://calf.sourceforge.net/plugins/RotarySpeaker#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix uiext: <http://lv2plug.in/ns/extensions/ui#> .
@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix lv2midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix lv2ctx: <http://lv2plug.in/ns/dev/contexts#> .
@prefix strport: <http://lv2plug.in/ns/dev/string-port#> .
@prefix pg: <http://lv2plug.in/ns/ext/port-groups#> .
@prefix ue: <http://lv2plug.in/ns/extensions/units#> .
@prefix epp: <http://lv2plug.in/ns/ext/port-props#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix param: <http://lv2plug.in/ns/ext/parameters#> .

<http://calf.sourceforge.net/plugins/gui/gtk2-gui>
    a uiext:GtkUI ;
    lv2:extensionData uiext:showInterface ;
    lv2:requiredFeature uiext:makeResident ;
    uiext:binary <calflv2gui.so> .

<http://calf.sourceforge.net/plugins/gui/gtk2-gui> uiext:portNotification [
    uiext:plugin <http://calf.sourceforge.net/plugins/RotarySpeaker> ;
    uiext:portIndex 10
] .

<http://calf.sourceforge.net/plugins/gui/gtk2-gui> uiext:portNotification [
    uiext:plugin <http://calf.sourceforge.net/plugins/RotarySpeaker> ;
    uiext:portIndex 11
] .

:in a pg:StereoGroup , pg:InputGroup ;
    lv2:symbol "in" ;
    rdfs:label "Input" .

:out a pg:StereoGroup , pg:OutputGroup ;
    lv2:symbol "out" ;
    rdfs:label "Output" .

<http://calf.sourceforge.net/plugins/RotarySpeaker> a lv2:Plugin ;
    a lv2:SimulatorPlugin ;
    doap:name "Calf Rotary Speaker" ;
    doap:maintainer [ foaf:name "Calf Studio Gear / Krzysztof Foltman" ; ] ;
    uiext:ui <http://calf.sourceforge.net/plugins/gui/gtk2-gui> ;
    lv2:optionalFeature <http://lv2plug.in/ns/ext/instance-access> ;
    lv2:optionalFeature <http://lv2plug.in/ns/ext/data-access> ;
    doap:license <http://usefulinc.com/doap/licenses/lgpl> ;
    dct:replaces <urn:ladspa:33923> ;
    lv2:optionalFeature epp:supportsStrictBounds ;
    lv2:optionalFeature lv2:hardRTCapable ;
    lv2:optionalFeature <http://lv2plug.in/ns/ext/urid#map> ;
    pg:mainInput :in ;
    pg:mainOutput :out ;
    lv2:port [
        a lv2:InputPort ;
        a lv2:AudioPort ;
        lv2:index 0 ;
        lv2:symbol "in_l" ;
        lv2:name "In L" ;
        lv2:designation pg:left ;
        pg:group :in ;
    ] , [
        a lv2:InputPort ;
        a lv2:AudioPort ;
        lv2:index 1 ;
        lv2:symbol "in_r" ;
        lv2:name "In R" ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:designation pg:right ;
        pg:group :in ;
    ] , [
        a lv2:OutputPort ;
        a lv2:AudioPort ;
        lv2:index 2 ;
        lv2:symbol "out_l" ;
        lv2:name "Out L" ;
        lv2:designation pg:left ;
        pg:group :out ;
    ] , [
        a lv2:OutputPort ;
        a lv2:AudioPort ;
        lv2:index 3 ;
        lv2:symbol "out_r" ;
        lv2:name "Out R" ;
        lv2:designation pg:right ;
        pg:group :out ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 4 ;
        lv2:symbol "vib_speed" ;
        lv2:name "Speed Mode" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:integer ;
        lv2:portProperty lv2:enumeration ;
        lv2:scalePoint [ rdfs:label "Off"; rdf:value 0 ] ;
        lv2:scalePoint [ rdfs:label "Chorale"; rdf:value 1 ] ;
        lv2:scalePoint [ rdfs:label "Tremolo"; rdf:value 2 ] ;
        lv2:scalePoint [ rdfs:label "HoldPedal"; rdf:value 3 ] ;
        lv2:scalePoint [ rdfs:label "ModWheel"; rdf:value 4 ] ;
        lv2:scalePoint [ rdfs:label "Manual"; rdf:value 5 ] ;
        lv2:default 5.00000 ;
        lv2:minimum 0.00000 ;
        lv2:maximum 5.00000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 5 ;
        lv2:symbol "spacing" ;
        lv2:name "Tap Spacing" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:default 0.500000 ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 6 ;
        lv2:symbol "shift" ;
        lv2:name "Tap Offset" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:default 0.500000 ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 7 ;
        lv2:symbol "mod_depth" ;
        lv2:name "FM Depth" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:default 0.450000 ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 8 ;
        lv2:symbol "treble_speed" ;
        lv2:name "Treble Motor" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty epp:logarithmic ;
        lv2:default 36.0000 ;
        lv2:minimum 10.0000 ;
        lv2:maximum 600.000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 9 ;
        lv2:symbol "bass_speed" ;
        lv2:name "Bass Motor" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty epp:logarithmic ;
        lv2:default 30.0000 ;
        lv2:minimum 10.0000 ;
        lv2:maximum 600.000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 10 ;
        lv2:symbol "mic_distance" ;
        lv2:name "Mic Distance" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:default 0.700000 ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 11 ;
        lv2:symbol "reflection" ;
        lv2:name "Reflection" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:default 0.300000 ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 12 ;
        lv2:symbol "am_depth" ;
        lv2:name "AM Depth" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:default 0.450000 ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 13 ;
        lv2:symbol "test" ;
        lv2:name "Test" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:default 0.00000 ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 14 ;
        lv2:symbol "meter_l" ;
        lv2:name "Low rotor" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 15 ;
        lv2:symbol "meter_h" ;
        lv2:name "High rotor" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:InputPort ;
        a atom:AtomPort ;
        lv2:index 16 ;
        lv2:symbol "midi_in" ;
        lv2:name "MIDI" ;
        lv2:portProperty lv2:connectionOptional ;
        atom:bufferType atom:Sequence ;
        atom:supports lv2midi:MidiEvent ;
    ]
.


